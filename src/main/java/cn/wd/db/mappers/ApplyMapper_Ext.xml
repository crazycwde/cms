  		<where>
  			state IS NULL OR state = ''
  		</where>
  </select>
  <select id="selectList" resultMap="BaseResultMap" parameterType="cn.wd.pojo.Apply" >
  		select * from j_apply 
  		<where>
  			state IS NULL OR state = ''
  		</where>
  </select>
  <select id="getCountY" resultType="java.lang.Integer" >
